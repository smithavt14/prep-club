<% @preps.each do |prep| %>
  <ul>
    <li>
      <%= link_to prep_path(prep) do %>
        <h3><%= prep.name %> </h3>
      <% end %>
      <p><%= prep.meal %></p>
      <p><%= prep.time %></p>
      <p><%= prep.location %></p>
    </li>
  </ul>
<% end %>
